{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "kadence/logo",
	"title": "Site Logo (Adv)",
	"category": "kadence-blocks",
	"textdomain": "kadence-blocks",
	"attributes": {
		"uniqueID": {
			"type": "string"
		},
		"showSiteTitle": {
			"type": "boolean",
			"default": true
		},
		"showSiteTagline": {
			"type": "boolean",
			"default": false
		},
		"layout": {
			"type": "string",
			"default": "logo-title"
		},
		"containerMaxWidth": {
			"type": "number"
		},
		"tabletContainerMaxWidth": {
			"type": "number"
		},
		"mobileContainerMaxWidth": {
			"type": "number"
		},
		"containerMaxWidthType": {
			"type": "string",
			"default": "px"
		}
	},
	"supports": {
		"anchor": true,
		"kbMetadata": true
	}
}
