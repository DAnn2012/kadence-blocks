{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "kadence/navigation",
	"title": "Navigation (Adv)",
	"category": "theme",
	"description": "An advanced navigation block.",
	"keywords": ["menu", "navigation", "links"],
	"textdomain": "default",
	"attributes": {
		"uniqueID": {
			"type": "string"
		},
		"id": {
			"type": "integer",
			"default": 0
		}
	},
	"supports": {
		"align": ["wide", "full"],
		"ariaLabel": true,
		"html": false
	},
	"usesContext": ["kadence/headerPostId", "kadence/headerIsSticky", "kadence/headerIsTransparent"]
}
