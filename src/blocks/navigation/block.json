{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "kadence/navigation",
	"title": "Navigation (Adv)",
	"category": "theme",
	"description": "An advanced navigation block.",
	"keywords": ["menu", "navigation", "links"],
	"textdomain": "default",
	"attributes": {
		"uniqueID": {
			"type": "string"
		},
		"id": {
			"type": "integer",
			"default": 0
		},
		"navigationOrientation": {
			"type": "string",
			"default": ""
		},
		"navigationSpacing": {
			"type": "array",
			"default": ["", "", "", ""]
		},
		"navigationSpacingUnit": {
			"type": "string",
			"default": "em"
		},
		"navigationStyle": {
			"type": "string",
			"default": ""
		},
		"navigationStretch": {
			"type": "boolean",
			"default": false
		},
		"navigationFillStretch": {
			"type": "boolean",
			"default": false
		}
	},
	"supports": {
		"align": ["wide", "full"],
		"ariaLabel": true,
		"html": false
	}
}
